import random

def task_3():
    lst_random = []
    n = int(input())
    n_min = int(input())
    n_max = int(input())
    for i in range(n):
        a = random.uniform(-10, 10)
        lst_random.append(a)
    count = 0
    for j in range(len(lst_random)):
        if int(lst_random[j]) >= n_min and int(lst_random[j]) < n_max:
            count = count + 1
    print(count)
task_3()
